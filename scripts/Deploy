echo "==== Value of CF_APP : [${CF_APP}]"
cf buildpacks
echo "----- END buildpacks------"
cf logs pipeline-1518642273 --recent
echo "----- END logs------"
ls -al
echo "==== Is the war file available - in ${ARCHIVE_DIR}"
ls -al ${ARCHIVE_DIR}
echo "==== Deploy the the war"
cf push "${CF_APP}" -p ${ARCHIVE_DIR}/StreamsProxy-v1.0.war
